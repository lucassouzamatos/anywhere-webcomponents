{
  "name": "anywhere-webcomponents",
  "version": "0.0.1",
  "homepage": "https://github.com/tiagoboeing/anywhere-webcomponents#readme",
  "description": "A UI based on web components to use in anywhere",
  "main": "dist/index.js",
  "module": "dist/custom-elements/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/custom-elements/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/anywhere-webcomponents/anywhere-webcomponents.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "changelog": "auto-changelog --template changelog-template.hbs -p -u --commit-limit false",
    "changelog.debug": "auto-changelog --template changelog-template.hbs -p --template json --output changelog-data.json"
  },
  "dependencies": {
    "@stencil/core": "^1.17.3",
    "@types/jest": "^26.0.10",
    "auto-changelog": "^2.2.0",
    "puppeteer-core": "^5.2.1"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tiagoboeing/anywhere-webcomponents/issues"
  },
  "repository": {
    "url": "https://github.com/tiagoboeing/anywhere-webcomponents"
  },
  "devDependencies": {
    "@types/puppeteer": "2.0.1",
    "jest": "26.0.1",
    "jest-cli": "26.0.1",
    "puppeteer": "2.1.1"
  },
  "auto-changelog": {
    "commitLimit": false,
    "unreleased": true,
    "issueUrl": "https://app.clickup.com/t/{id}",
    "replaceText": {
      "[Ff]eature:": "",
      "[Ff]ix:": "",
      "[Bb]reak:": "",
      "^\\#\\d": "[$1](https://github.com/tiagoboeing/anywhere-webcomponents/issues/$1) - "
    },
    "includeBranch": [
      "develop",
      "master"
    ]
  }
}
